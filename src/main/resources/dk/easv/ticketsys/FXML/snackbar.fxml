<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox fx:id="hboxSnack" alignment="CENTER_LEFT" spacing="5.0" stylesheets="@../CSS/homeStyle.css" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dk.easv.ticketsys.PL.SnackbarController">
   <children>
      <ImageView fx:id="imgView" fitHeight="5.0" fitWidth="1.0" pickOnBounds="true" preserveRatio="true">
         <HBox.margin>
            <Insets left="5.0" right="5.0" />
         </HBox.margin></ImageView>
      <VBox prefHeight="50.0" prefWidth="200.0">
         <children>
            <Label fx:id="txtTitle" styleClass="alert-title" text="Label" />
            <Label fx:id="txtSnack" styleClass="alert-body" text="Label" />
         </children>
      </VBox>
   </children>
</HBox>
